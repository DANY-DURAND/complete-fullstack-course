<section>
    <h2>Latest Posts</h2>
    <% if (locals.data) { %>
        <% data.forEach(el => { %>
            <h3><%= el.title %></h3>
            <p> <%= el.content %></p>
            <button type="submit">Read more</button>
        <% }) %>
    <% } %>
        
</section>


